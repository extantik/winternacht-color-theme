    "name": "Winternacht Meltdown",
    "semanticHighlighting": true,

    "semanticTokenColors": {
        "*.builtin": {
            "foreground": "#ede29b"
        },
        "*.builtin:robotframework": {
            "foreground": "#edca9c"
        },
        "*.declaration:robotframework": {
            "fontStyle": "bold underline",
        },
        "*.typeHint": {
            "foreground": "#edca9c"
        },
        "class.declaration": {
            "fontStyle": "bold",
            "foreground": "#e3a95f"
        },
        "method": {
            "foreground": "#ede29b"
        },
        "method.declaration": {
            "fontStyle": "bold",
            "foreground": "#ede29b"
        },
        "namespace": {
            "foreground": "#edca9c"
        },
        "parameter": {
            "foreground": "#ece8df"
        },
        "property": {
            "foreground": "#ede29b"
        },
        "selfParameter": {
            "foreground": "#edca9c"
        },
        "variable": {
            "foreground": "#e8e4d9"
        }
    },
    "tokenColors": [
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#e35f5f"
            }
        },
        {
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#ed9bc7"
            }
        },
        {
            "scope": [
                "meta.method.declaration storage.type.ts",
                "meta.method.declaration storage.type.tsx"
            ],
            "settings": {
                "foreground": "#e3a95f"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#ede29b"
            }
        },
        {
            "scope": [
                "constant.language",
                "variable.language.this",
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#edca9c"
            }
        },
        {
            "scope": "markup.list",
            "settings": {
                "foreground": "#e3a95f"
            }
        },
        {
            "scope": "markup",
            "settings": {
                "foreground": "#ede29b"
            }
        },
        {
            "scope": [
                "punctuation.definition.comment",
                "comment"
            ],
            "settings": {
                "foreground": "#abc8de60"
            }
        },
        {
            "scope": [
                "meta.paragraph.markdown",
                "source.dockerfile",
                "ref.matchtext"
