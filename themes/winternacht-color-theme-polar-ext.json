    "name": "Winternacht Polar Ext",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "*.builtin": {
            "foreground": "#ffb3cc"
        },
        "*.builtin:robotframework": {
            "foreground": "#42c0ff"
        },
        "*.declaration:robotframework": {
            "fontStyle": "bold",
            "foreground": "#ff8ab1"
        },
        "*.typeHint": {
            "foreground": "#ffb3cc"
        },
        "class.declaration": {
            "fontStyle": "bold",
            "foreground": "#ff8aff"
        },
        "method": {
            "foreground": "#8ad8ff"
        },
        "method.declaration": {
            "fontStyle": "bold",
            "foreground": "#42c0ff"
        },
        "namespace": {
            "foreground": "#ed9bed"
        },
        "parameter": {
            "foreground": "#9bd2ed"
        },
        "property": {
            "foreground": "#8affca"
        },
        "selfParameter": {
            "foreground": "#42c0ff"
        },
        "variable": {
            "foreground": "#c7e6f5"
        }
    },
    "tokenColors": [
        {
            "scope": [
                "entity"
            ],
            "settings": {
                "foreground": "#ed9bed"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#42ffff"
            }
        },
        {
            "scope": "constant",
            "settings": {
                "foreground": "#f5c7f5"
            }
        },
        {
            "scope": "constant.other",
            "settings": {
                "foreground": "#c7f5e0"
            }
        },
        {
            "scope": "support",
            "settings": {
                "foreground": "#9beded"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#c7e6f5"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#c7f5e0"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#ddffb3"
            }
        },
        {
            "scope": [
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end"
            ],
            "settings": {
                "foreground": "#c7f5e0"
            }
        },
        {
            "scope": "meta",
            "settings": {
                "foreground": "#c7c7f5"
            }
        },
        {
            "scope": "string.unquoted",
            "settings": {
                "foreground": "#c7f5e0"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#ff4242"
            }
        },
        {
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#ffaa42"
            }
        },
        {
            "scope": [
                "variable.other.meta.import.js",
                "meta.import.js variable.other",
                "variable.other.meta.export.js",
                "variable.other.readwrite.js",
                "meta.export.js variable.other",
                "variable.other.readwrite.alias.ts",
                "variable.other.object.js",
                "variable.other.readwrite.alias.tsx",
                "variable.other.readwrite.ts",
                "variable.other.readwrite.tsx",
                "variable.other.object.ts",
                "variable.other.object.tsx",
                "variable.object.property.ts",
                "variable.object.property.tsx",
                "variable.other.ts",
                "variable.other.tsx",
                "variable.tsx",
                "variable.ts"
            ],
            "settings": {
                "foreground": "#c7f5f5"
            }
        },
        {
            "scope": [
                "meta.var.expr",
                "meta.class meta.method.declaration meta.var.expr storage.type.js",
                "storage.type.property.js",
                "storage.type.property.ts",
                "entity.name.type.tsx",
                "meta.class entity.name.type.class.tsx",
                "entity.name.type.module.tsx"
            ],
            "settings": {
                "foreground": "#c7f5e0"
            }
        },
        {
            "scope": [
                "meta.method.declaration storage.type.ts",
                "meta.method.declaration storage.type.tsx"
            ],
            "settings": {
                "foreground": "#42c0ff"
            }
        },
        {
            "scope": "variable.other.property",
            "settings": {
                "foreground": "#c7f5e0"
            }
        },
        {
            "scope": [
                "variable.instance",
                "variable.other.instance",
                "variable.readwrite.instance",
                "variable.other.readwrite.instance"
            ],
            "settings": {
                "foreground": "#a3f1f5"
            }
        },
        {
            "scope": "variable.other.constant",
            "settings": {
                "foreground": "#9beded"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#8ad8ff"
            }
        },
        {
            "scope": [
                "constant.language"
            ],
            "settings": {
                "foreground": "#f5c7f5"
            }
        },
        {
            "scope": "markup.list",
            "settings": {
                "foreground": "#c7f5e0"
            }
        },
        {
            "scope": [
                "markup.inline.raw.string.markdown",
                "markup.fenced_code.block.markdown"
            ],
            "settings": {
                "foreground": "#c7f5e0"
            }
        },
        {
            "scope": "markup",
            "settings": {
                "foreground": "#c7e6f5"
            }
        },
        {
            "name": "Markdown Punctuation",
            "scope": [
                "punctuation.definition.bold.markdown",
                "punctuation.definition.italic.markdown",
                "punctuation.definition.underline.markdown",
                "punctuation.definition.metadata.markdown"
            ],
            "settings": {
                "foreground": "#f76e8e"
            }
        },
        {
            "scope": [
                "punctuation.definition.comment",
                "comment"
            ],
            "settings": {
                "foreground": "#e5ecf060"
            }
        },
        {
            "scope": [
                "meta.paragraph.markdown",
                "source.dockerfile",
                "ref.matchtext"
            ],
            "settings": {
                "foreground": "#c7f5f5"
            }
        },
        {
            "scope": [
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#9bedc8"
            }
        },
        {
            "scope": [ // declarations
                "storage.type",
                "entity.name.section.markdown",
                "punctuation.definition.heading.markdown",
                "markup.italic.markdown",
                "markup.bold.markdown",
                "markup.underline.markdown"
            ],
            "settings": {
                "foreground": "#ffb3cc"
            }
        },
        {
            "scope": [ // self
                "support.type.property-name.json",
                "entity.name.type.module",
                "variable.parameter.function",
                "entity.name.class",
                "entity.name.section",
                "markup.underline.link.image.markdown",
                "markup.underline.link.markdown",
                "variable.language.this",
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#42c0ff"
            }
        },
        {
            "scope": [
                "meta.function",
                "keyword.control",
                "support.function",
                "constant.character"
            ],
            "settings": {
                "foreground": "#ffb3cc"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted",
                "string.quoted.double.json"
            ],
            "settings": {
                "foreground": "#c7f5e0"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#42c0ff"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#ffff42"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#ff4242"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#42ff42"
            }
        }
    ],
    "type": "dark"
}