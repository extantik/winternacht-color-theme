    "name": "Winternacht Polar Ext",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "*.builtin": {
            "foreground": "#f986a1"
        },
        "*.builtin:robotframework": {
            "foreground": "#2cbdfc"
        },
        "*.declaration:robotframework": {
            "fontStyle": "bold",
            "foreground": "#f76e8e"
        },
        "*.typeHint": {
            "foreground": "#f986a1"
        },
        "class.declaration": {
            "fontStyle": "bold",
            "foreground": "#13b6fb"
        },
        "method": {
            "foreground": "#77d5fd"
        },
        "method.declaration": {
            "fontStyle": "bold",
            "foreground": "#45c5fc"
        },
        "namespace": {
            "foreground": "#2cbdfc"
        },
        "parameter": {
            "foreground": "#a3dcf5"
        },
        "property": {
            "foreground": "#86f3f9"
        },
        "selfParameter": {
            "foreground": "#45c5fc"
        },
        "variable": {
            "foreground": "#cdeffe"
        }
    },
    "tokenColors": [
        {
            "scope": [
                "entity"
            ],
            "settings": {
                "foreground": "#2cbdfc"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#09e3fb"
            }
        },
        {
            "scope": "constant",
            "settings": {
                "foreground": "#c3d1fe"
            }
        },
        {
            "scope": "constant.other",
            "settings": {
                "foreground": "#82d9cc"
            }
        },
        {
            "scope": "support",
            "settings": {
                "foreground": "#3ed3db"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#cdeffe"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#b4fdf2"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#ddc340"
            }
        },
        {
            "scope": [
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end"
            ],
            "settings": {
                "foreground": "#82d9cc"
            }
        },
        {
            "scope": "meta",
            "settings": {
                "foreground": "#b4c5fd"
            }
        },
        {
            "scope": "string.unquoted",
            "settings": {
                "foreground": "#b4fdf2"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#fa0008"
            }
        },
        {
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#ddc340"
            }
        },
        {
            "scope": [
                "variable.other.meta.import.js",
                "meta.import.js variable.other",
                "variable.other.meta.export.js",
                "variable.other.readwrite.js",
                "meta.export.js variable.other",
                "variable.other.readwrite.alias.ts",
                "variable.other.object.js",
                "variable.other.readwrite.alias.tsx",
                "variable.other.readwrite.ts",
                "variable.other.readwrite.tsx",
                "variable.other.object.ts",
                "variable.other.object.tsx",
                "variable.object.property.ts",
                "variable.object.property.tsx",
                "variable.other.ts",
                "variable.other.tsx",
                "variable.tsx",
                "variable.ts"
            ],
            "settings": {
                "foreground": "#cffcf6"
            }
        },
        {
            "scope": [
                "meta.var.expr",
                "meta.class meta.method.declaration meta.var.expr storage.type.js",
                "storage.type.property.js",
                "storage.type.property.ts",
                "entity.name.type.tsx",
                "meta.class entity.name.type.class.tsx",
                "entity.name.type.module.tsx"
            ],
            "settings": {
                "foreground": "#b4fdf2"
            }
        },
        {
            "scope": [
                "meta.method.declaration storage.type.ts",
                "meta.method.declaration storage.type.tsx"
            ],
            "settings": {
                "foreground": "#45c5fc"
            }
        },
        {
            "scope": "variable.other.property",
            "settings": {
                "foreground": "#b4fdf2"
            }
        },
        {
            "scope": [
                "variable.instance",
                "variable.other.instance",
                "variable.readwrite.instance",
                "variable.other.readwrite.instance"
            ],
            "settings": {
                "foreground": "#a3f1f5"
            }
        },
        {
            "scope": "variable.other.constant",
            "settings": {
                "foreground": "#a9e5dc"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#77d5fd"
            }
        },
        {
            "scope": [
                "constant.language"
            ],
            "settings": {
                "foreground": "#c3d1fe"
            }
        },
        {
            "scope": "markup.list",
            "settings": {
                "foreground": "#82d9cc"
            }
        },
        {
            "scope": [
                "markup.inline.raw.string.markdown",
                "markup.fenced_code.block.markdown"
            ],
            "settings": {
                "foreground": "#82d9cc"
            }
        },
        {
            "scope": "markup",
            "settings": {
                "foreground": "#cffcf6"
            }
        },
        {
            "name": "Markdown Punctuation",
            "scope": [
                "punctuation.definition.bold.markdown",
                "punctuation.definition.italic.markdown",
                "punctuation.definition.underline.markdown",
                "punctuation.definition.metadata.markdown"
            ],
            "settings": {
                "foreground": "#f76e8e"
            }
        },
        {
            "scope": [
                "punctuation.definition.comment",
                "comment"
            ],
            "settings": {
                "foreground": "#e5ecf060"
            }
        },
        {
            "scope": [
                "meta.paragraph.markdown",
                "source.dockerfile",
                "ref.matchtext"
            ],
            "settings": {
                "foreground": "#cffcf6"
            }
        },
        {
            "scope": [
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#a9e5dc"
            }
        },
        {
            "scope": [ // declarations
                "storage.type",
                "entity.name.section.markdown",
                "punctuation.definition.heading.markdown",
                "markup.italic.markdown",
                "markup.bold.markdown",
                "markup.underline.markdown"
            ],
            "settings": {
                "foreground": "#f986a1"
            }
        },
        {
            "scope": [ // self
                "support.type.property-name.json",
                "entity.name.type.module",
                "variable.parameter.function",
                "entity.name.class",
                "entity.name.section",
                "markup.underline.link.image.markdown",
                "markup.underline.link.markdown",
                "variable.language.this",
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#45c5fc"
            }
        },
        {
            "scope": [
                "keyword.operator.arithmetic",
                "keyword.operator.assignment",
                "keyword.operator.logical",
                "keyword.operator.comparison",
                "punctuation.separator"
            ],
            "settings": {
                "foreground": "#82d9cc"
            }
        },
        {
            "scope": [
                "meta.function",
                "keyword.control",
                "support.function",
                "constant.character"
            ],
            "settings": {
                "foreground": "#fbb6c5"
            }
        },
        {
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#9be0fd"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#fdfb72"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted",
                "string.quoted.double.json"
            ],
            "settings": {
                "foreground": "#b4fdf2"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#45c5fc"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#fdfb72"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#f53d68"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#24f9c1"
            }
        }
    ],
    "type": "dark"
}