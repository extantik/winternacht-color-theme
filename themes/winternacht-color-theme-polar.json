    "name": "Winternacht Melt",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "*.builtin": {
            "foreground": "#f5c7d6"
        },
        "*.builtin:robotframework": {
            "foreground": "#5fb7e3"
        },
        "*.declaration:robotframework": {
            "fontStyle": "bold",
            "foreground": "#ed9bb7"
        },
        "*.typeHint": {
            "foreground": "#e2a7bb"
        },
        "class.declaration": {
            "fontStyle": "bold",
            "foreground": "#ed9bc8"
        },
        "method": {
            "foreground": "#9bd2ed"
        },
        "method.declaration": {
            "fontStyle": "bold",
            "foreground": "#5fb7e3"
        },
        "namespace": {
            "foreground": "#e2a7c7"
        },
        "parameter": {
            "foreground": "#cdcdee"
        },
        "property": {
            "foreground": "#9bd2ed"
        },
        "selfParameter": {
            "foreground": "#5fb7e3"
        },
        "variable": {
            "foreground": "#c3e3f3"
        }
    },
    "tokenColors": [
        {
            "scope": [
                "entity"
            ],
            "settings": {
                "foreground": "#ed9bc8"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#9beded"
            }
        },
        {
            "scope": "constant",
            "settings": {
                "foreground": "#f5c7e0"
            }
        },
        {
            "scope": "constant.other",
            "settings": {
                "foreground": "#cdeee0"
            }
        },
        {
            "scope": "support",
            "settings": {
                "foreground": "#a7e2d5"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#d4e1e8"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#cdeee0"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#e2c7a7"
            }
        },
        {
            "scope": [
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end"
            ],
            "settings": {
                "foreground": "#a7e2c7"
            }
        },
        {
            "scope": "meta",
            "settings": {
                "foreground": "#e8d4e8"
            }
        },
        {
            "scope": "string.unquoted",
            "settings": {
                "foreground": "#cdeee0"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#e35f5f"
            }
        },
        {
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#e3a75f"
            }
        },
        {
            "scope": [
                "variable.other.meta.import.js",
                "meta.import.js variable.other",
                "variable.other.meta.export.js",
                "variable.other.readwrite.js",
                "meta.export.js variable.other",
                "variable.other.readwrite.alias.ts",
                "variable.other.object.js",
                "variable.other.readwrite.alias.tsx",
                "variable.other.readwrite.ts",
                "variable.other.readwrite.tsx",
                "variable.other.object.ts",
                "variable.other.object.tsx",
                "variable.object.property.ts",
                "variable.object.property.tsx",
                "variable.other.ts",
                "variable.other.tsx",
                "variable.tsx",
                "variable.ts"
            ],
            "settings": {
                "foreground": "#d4e1e8"
            }
        },
        {
            "scope": [
                "meta.var.expr",
                "meta.class meta.method.declaration meta.var.expr storage.type.js",
                "storage.type.property.js",
                "storage.type.property.ts",
                "entity.name.type.tsx",
                "meta.class entity.name.type.class.tsx",
                "entity.name.type.module.tsx"
            ],
            "settings": {
                "foreground": "#cdeee0"
            }
        },
        {
            "scope": [
                "meta.method.declaration storage.type.ts",
                "meta.method.declaration storage.type.tsx"
            ],
            "settings": {
                "foreground": "#71b0d0"
            }
        },
        {
            "scope": "variable.other.property",
            "settings": {
                "foreground": "#cdeee0"
            }
        },
        {
            "scope": [
                "variable.instance",
                "variable.other.instance",
                "variable.readwrite.instance",
                "variable.other.readwrite.instance"
            ],
            "settings": {
                "foreground": "#a3f1f5"
            }
        },
        {
            "scope": "variable.other.constant",
            "settings": {
                "foreground": "#e2c7a7"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#9bd2ed"
            }
        },
        {
            "scope": [
                "constant.language"
            ],
            "settings": {
                "foreground": "#f5c7e0"
            }
        },
        {
            "scope": "markup.list",
            "settings": {
                "foreground": "#e2c7a7"
            }
        },
        {
            "scope": [
                "markup.inline.raw.string.markdown",
                "markup.fenced_code.block.markdown"
            ],
            "settings": {
                "foreground": "#cdeee0"
            }
        },
        {
            "scope": "markup",
            "settings": {
                "foreground": "#c7e6f5"
            }
        },
        {
            "name": "Markdown Punctuation",
            "scope": [
                "punctuation.definition.bold.markdown",
                "punctuation.definition.italic.markdown",
                "punctuation.definition.underline.markdown",
                "punctuation.definition.metadata.markdown"
            ],
            "settings": {
                "foreground": "#ed9bb7"
            }
        },
        {
            "scope": [
                "punctuation.definition.comment",
                "comment"
            ],
            "settings": {
                "foreground": "#e5ecf060"
            }
        },
        {
            "scope": [
                "meta.paragraph.markdown",
                "source.dockerfile",
                "ref.matchtext"
            ],
            "settings": {
                "foreground": "#c7f5f5"
            }
        },
        {
            "scope": [
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#a7e2e2"
            }
        },
        {
            "scope": [ // declarations
                "storage.type",
                "entity.name.section.markdown",
                "punctuation.definition.heading.markdown",
                "markup.italic.markdown",
                "markup.bold.markdown",
                "markup.underline.markdown"
            ],
            "settings": {
                "foreground": "#ed9bb7"
            }
        },
        {
            "scope": [
                "support.type.property-name.json",
                "entity.name.type.module",
                "variable.parameter.function",
                "entity.name.class",
                "entity.name.section",
                "markup.underline.link.image.markdown",
                "markup.underline.link.markdown",
                "variable.language.this",
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#5fb7e3"
            }
        },
        {
            "scope": [
                "meta.function",
                "keyword.control",
                "support.function",
                "constant.character"
            ],
            "settings": {
                "foreground": "#e2a7bb"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted",
                "string.quoted.double.json"
            ],
            "settings": {
                "foreground": "#cdeee0"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#71b0d0"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#e3e35f"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#e35f5f"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#5fe3c6"
            }
        }
    ],
    "type": "dark"
}